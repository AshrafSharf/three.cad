{
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "export NODE_ENV=production && webpack --config webpack.prod.js",
    "get-bundle-size": "webpack --profile --json > stats.json --config webpack.prod.js && webpack-bundle-analyzer stats.json dist/",
    "deploy": "export NODE_ENV=production && webpack --config webpack.prod.js && gh-pages -d dist -t true -m"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.13",
    "@tailwindcss/jit": "^0.1.18",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.3",
    "gh-pages": "^3.1.0",
    "mini-css-extract-plugin": "^1.4.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "stats-js": "^1.0.1",
    "tailwindcss": "^2.0.4",
    "three": "^0.126.1",
    "webpack": "^5.26.3",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
